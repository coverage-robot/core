{
    "name": "coverage-robot/message",
    "description": "Decoupled queue messages usable across the services",
    "type": "library",
    "version": "1.0.0",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "coverage-robot/event": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Packages\\Message\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../event"
        },
        {
            "type": "path",
            "url": "../models"
        },
        {
            "type": "path",
            "url": "../contracts"
        }
    ]
}
